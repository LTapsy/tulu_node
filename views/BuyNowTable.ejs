<script>
    var dealership = "";
    var price = "";
    var currVehicle = "";
    var address = "";

    window.onload = function () {

        var hash;
        var hashes = window.location.href.slice(window.location.href.indexOf("?") + 1).split("&");
        
        for (var i = 0; i < hashes.length; i++) {
            hash = hashes[i].split("=");
            if (hash[0] === "dealership") {
                dealership = hash[1];
            } else if (hash[0] === "price") {
                price = hash[1];
            }else if (hash[0] === "vehicle") {
                currVehicle = hash[1];
            }else if (hash[0] === "address") {
                address = hash[1];
            }
        }

        dealership = dealership.replace(/%20/g, " ");
        address = address.replace(/%20/g, " ");
        currVehicle = currVehicle.replace(/%20/g, " ");

        
        console.log(dealership);
        console.log(price);
        console.log(currVehicle);
        console.log(address);


        var html = "";
        html+='<p>Dealership : '+dealership+'</p>';
        html+='<p>Address : '+address+'</p>';
        html+='<p>Vehicle :'+currVehicle+'</p>';
        html+='<p>Price : $'+price+'</p>';
        $('.header').append(html);
    }

</script>
