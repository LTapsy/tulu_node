@using System.Security.Claims
@using Tulu.Objects.DataObjects
@{
    ViewBag.Title = "Claims";
    var data = (Users)ViewData["User"];
}
<h2>@ViewBag.Title</h2>

<h3>@data.ToString()</h3>
<input type="button" class="btn" value="Malik Test" onclick="TestMe();"/>
<h4>Claims Present in the Claims Identity:</h4>

<table class="table-hover claim-table">
    <tr>
        <th class="claim-type claim-data claim-head">Claim Type</th>
        <th class="claim-data claim-head">Claim Value</th>
    </tr>

    @foreach (Claim claim in ClaimsPrincipal.Current.Claims)
    {
        <tr>
            <td class="claim-type claim-data">@claim.Type</td>
            <td class="claim-data">@claim.Value</td>
        </tr>
    }
</table>