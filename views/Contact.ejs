<script src="Js/sideMenu.js"></script>
<script src="Js/privileges.js"></script>


    @if (User.Identity.IsAuthenticated) {
        <Script>
            GetUser();
        </Script>      
    }else{
        <Script>
            guestprivileges();
        </Script> 
    }
