            </ul>

        
	
        </section>

        <!-- Third Section -->
        <section class="thirdSection">
            <h2 class="influencerHeading">TOP TULUS</h2>
            <ul class="influencerList">
                <li class="influencerListItems">
                    <div class="influencerImage">
                        <a href="home/profile">
                            <img src="images/jolo.jpeg" alt="">
                        </a>
                    </div>
                    <h3 class="influencerName">Tulu Jo</h3>
                </li>
                <li class="influencerListItems">
                    <div class="influencerImage">
                        <img src="images/shae.jpg" alt="">
                    </div>
                    <h3 class="influencerName">Tulu Shae</h3>
                </li>
                <li class="influencerListItems">
                    <div class="influencerImage">
