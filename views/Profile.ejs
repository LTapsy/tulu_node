<body>
    <nav id="Nav">
        <a href="/">
            <img src="images/tulu-logo_white.png" alt="">
        </a>
        <div class="menuButton" onclick="slideMenu()">
            <i class="fas fa-bars fa-2x menuBtn"></i>
            <i class="fas fa-times fa-2x closeMenuBtn"></i>
        </div>
    </nav>

    <div id="SideMenu" class="sideMenu">
        <ul class="menu">
            <%- include partials/sidemenu %>
            
        </ul>
        <p class="loggedInAs"></p>
    </div>


    <div class="container body-content">

        <div class="header" id="Header">
            <div class="banner">
            </div>
            <div class="userInfo">
                <div class="userImage" id="userImage">
                    <img src="images/jolo.jpeg" alt="" class="face">
                </div>
                <h3 class="userName">
                    <%= fName%> <%= lName%>
                </h3>
                <p class="userBio">
                    <%= bio%>
                </p>
                
                <ul class="socialMediaAccounts">
                    <li>
                        <a href="<%= facebook%>" target="_blank">
                            <i class="fab fa-facebook fa-2x"></i>
                        </a>
                    </li>
                    <li>
                        <a href="<%= instagram%>" target="_blank">
                            <i class="fab fa-instagram fa-2x"></i>
                        </a>
                    </li>
                    <li>
                        <a href="<%= linkedin%>" target="_blank">
                            <i class="fab fa-linkedin-in fa-2x"></i>
                        </a>
                    </li>
                </ul>
                <div class="subInfo">
                    <h4>
                    Favourite Car:
                    </h4>
                    <p class="favouriteCar">
                        <%= favoriteCar%>
                    </p>
                    <h4>
                    Current Car:
                    </h4>
                    <p class="currentCar">
                        <%= currentCar%>
                    </p>
                    <h4>
                        Location: 
                    </h4>
                    <p class="location">
                        <%= role%>
                    </p>
                </div>
                <a href="/Account/EditProfile">
                    <button class="editProfileBtn">
                        Edit Profile
                    </button>
                </a>
            </div>

        </div>

